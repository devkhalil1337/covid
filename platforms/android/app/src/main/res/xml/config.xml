<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coronapk.pk" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>StatsViewer</name>
    <description>
        A utility app for viewing public data trends and statistics.
    </description>
    <author email="dev1337@gmail.com" href="#">
        Visionary Sols
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="android-targetSdkVersion" value="34" />
</widget>
